generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum Role {
  USER
  PARTENAIRE
  
}

model Contact {
  id    Int     @id @default(autoincrement())
  nom String 
  prenom String
  role    Role?
  email String? 
  tel String? 
  date_creation DateTime @default(now())
}

